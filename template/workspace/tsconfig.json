{
  "display": "React Native",
  "_version": "0.0.1",
  "compilerOptions": {
    "removeComments": true,
    "composite": true,
    "declaration": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "module": "ESNext",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noStrictGenericChecks": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ES6",
    "allowJs": true,
    "baseUrl": "./src",
    "paths": {
      "@poeta/*": ["@poeta/shared/build/*"],
      "@poeta/react-native/*": ["@poeta/react-native-shared/build/*"],
      "@/*": ["./src/*"]
    }
  },
  "exclude": [
    "build",
    "node_modules",
    "**/test/*.ts",
    "**/__mock__/*.ts",
    "packages"
  ]
}
